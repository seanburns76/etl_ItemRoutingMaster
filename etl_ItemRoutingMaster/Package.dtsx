                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  
irkitl AS PartNumber,
trim(irmmcu) AS Branch,
trim(irmcu) AS WorkCenter ,
irdsc1 AS Description,
irtrt AS Routing,
iropsq/100 AS OpSeq,
irtimb AS TimeBase,
CASE
	WHEN irtimb = 'U' THEN 1
	WHEN irtimb = '1' THEN .1
	WHEN irtimb = '2' THEN .01
	WHEN irtimb = '3' THEN .001
	WHEN irtimb = '4' THEN .0001 END AS TimeBaseValue ,
iruom AS UnitOfMeasure,
irrunm AS StandardMachine,
irrunl AS StandardLabor ,
irsetl AS SetUpLabor,
irsetc AS StandardCrew,
irefff AS EffectiveFrom,
irefft AS EffectiveTo,
irupmj AS UpdateDate




